{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "This is a basic browser extension.",
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon":"icon.jpg"
    },
    "icons": {
      "128": "icon.jpg"
    }
  }